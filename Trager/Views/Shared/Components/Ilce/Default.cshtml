@model List<Trager.Models.Ilce>

<select onchange="this.form.submit" name="Id" id="ilce" class="form-control">
    <option value="">İlce</option>
    @if (Model != null)
    {
        foreach (var ilce in Model)
        {
            <option value="@ilce.Id">@ilce.Ad </option>
        }
    }

</select>
<span class="text-danger"></span>
