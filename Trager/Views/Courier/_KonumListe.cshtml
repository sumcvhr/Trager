
@model CourierHomeVM
<div class="col-lg-12 col-md-12 col-sm-12">
    <div class="row card card-body ml-0" style="max-width:100%">
        <div class="card-body">
            <div class="border-dark p-2 mb-3">
                <h5 class="mb-4"> Şuan Buralarda Çalışıyorsunuz </h5>
                <div class=" align-items-start row">
                  
                        @foreach (var Location in Model.Konum)
                        {

                        
                            <div class="profile-header-info col-lg-8 col-md-8 col-sm-8">
                                <p class="m-t-sm">@Location.Ilce.Il.Ad - @Location.Ilce.Ad </p>
                            </div>
                            <div class="col-md-3 col-lg-3 col-sm-3 ml-3">
                                <a href="/Courier/LocationDelete/@Location.Id" id="bitir" class="btn btn-danger" style="min-width:50%">Bitir</a>
                            </div>
                            <div class="my-1 ml-1" style="border-bottom: 1px solid #00000042; width: 90%;"></div>
                       

                        }

                </div>


            </div>

        </div>
    </div>
</div>
